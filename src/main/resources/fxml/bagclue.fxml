<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>

<AnchorPane prefHeight="599.0" prefWidth="789.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nz.ac.auckland.se206.controllers.BagClueController">
   <children>
      <ImageView fitHeight="605.0" fitWidth="818.0" layoutY="-4.0" preserveRatio="true">
         <image>
            <Image url="@../images/lowerlayerbag.png" />
         </image>
      </ImageView>
      <ImageView fx:id="bagzipper" fitHeight="556.0" fitWidth="174.0" layoutX="486.0" layoutY="166.0" preserveRatio="true">
         <image>
            <Image url="@../images/movingzipper.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="605.0" fitWidth="818.0" layoutY="-4.0" preserveRatio="true">
         <image>
            <Image url="@../images/toplayerbag.png" />
         </image>
      </ImageView>
      <Rectangle fx:id="rectZipper" arcHeight="5.0" arcWidth="5.0" fill="#194f7b" focusTraversable="true" height="44.0" layoutX="511.0" layoutY="398.0" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="25.0">
         <cursor>
            <Cursor fx:constant="MOVE" />
         </cursor>
      </Rectangle>
      <Rectangle fx:id="clickInto" arcHeight="5.0" arcWidth="5.0" fill="#194f7b" focusTraversable="true" height="116.0" layoutX="509.0" layoutY="304.0" onMouseClicked="#onOpenBag" opacity="0.0" stroke="BLACK" strokeType="INSIDE" width="19.0">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Rectangle>
      <Button fx:id="btnExit" layoutX="644.0" layoutY="28.0" mnemonicParsing="false" onAction="#onExitClue" prefHeight="38.0" prefWidth="103.0" text="BACK" />
      <ImageView fx:id="zoomedBagStuff" fitHeight="605.0" fitWidth="792.0" onMouseClicked="#closeBagStuff">
         <image>
            <Image url="@../images/thinginbag.png" />
         </image>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
   </children>
</AnchorPane>
